<div class="file">
  <h1>
    <span class="glyphicon gliphicon-folder-open"></span>
    <%= file.filename %>
    <small class="text-muted">
      <%= file.extension %>
    </small>
  </h1>
  <div class="preview">
    <% case file.type %>
    <% when 'image' %>
    <% when 'model' %>
    <% else %>
    <% end %>
  </div>
  <table class='table table-condensed table-striped'>
    <% file.metadata.each_pair do |key, value| %>
      <tr>
        <td><strong><%= key %></strong></td>
        <td><%= value %></td>
      </tr>
    <% end %>
  </table>
</div>

